@model Voo

@{
    ViewData["Title"] = "Cadastro de Voo";
}

<h1>Cadastro</h1>

@if (TempData["msg"] != null)
{
    <div class="alert alert-success">
        @TempData["msg"]
    </div>
}

<form asp-action="cadastrar">
    
    <partial name="_Form"/>

    <input type="submit" value="Cadastrar" class="btn btn-primary" />
</form>